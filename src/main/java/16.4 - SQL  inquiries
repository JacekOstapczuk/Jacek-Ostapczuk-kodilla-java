SELECT NAME, DESCRIPTIONOFISSUES FROM issues
JOIN issueslists i on i.ID = issues.ISSUESLIST_ID;

SELECT  FIRSTNAME, LASTNAME, DESCRIPTIONOFISSUES FROM issues
JOIN users u on u.ID = issues.USER_ID_ASSIGNEDTO;


SELECT  FIRSTNAME, LASTNAME,  COUNT(*) AS ILOŚĆ_PRZYDZIELONYCH_ZADAŃ FROM issues
JOIN users u on u.ID = issues.USER_ID_ASSIGNEDTO;